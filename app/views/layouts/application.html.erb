<!DOCTYPE html>
<html>
  <head>
    <title>CsvParser</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="page-wrap">
      <%= render "shared/header" %>

      <% if notice %>
        <p class="notice flash-message success"><%= notice %></p>
      <% end %>
      <% if alert %>
        <p class="alert flash-message error"><%= alert %></p>
      <% end %>
      <%= yield %>
    </div>

    <footer>
      <!--<span>&copy 2016. All Rights Reserved.</span>-->
    </footer>
  </body>
</html>
