//= depend_on_asset "grid-paper.jpg"
@import "mixins";

#user-show {
  @include gridPaperBackground;
  background: url('<%= asset_path("grid-paper.jpg") %> ') repeat;
  height: 800px;

  .page-top-section {
    text-align: left;
  }

  .wrapper-seventy {
    width: 70%;
    margin: 0 auto;

    .tasks-wrapper {
      a {
        text-decoration: underline;
      }

      .user-task-group {
        padding-bottom: 20px;
      }
    }
  }
}
